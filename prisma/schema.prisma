generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model box {
  id          Int       @id @default(autoincrement()) @db.UnsignedInt
  cesta_nome  String?   @db.VarChar(255)
  preco       Float?    @db.Float
  image_src   String?   @db.VarChar(255)
  catalogo_id Int?      @db.UnsignedInt
  catalogo    catalogo? @relation(fields: [catalogo_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "box_catalogo_id_foreign")
  item        item[]

  @@index([catalogo_id], map: "box_catalogo_id_foreign")
}

model catalogo {
  id     Int     @id @default(autoincrement()) @db.UnsignedInt
  titulo String? @db.VarChar(255)
  box    box[]
}

model item {
  id        Int     @id @default(autoincrement()) @db.UnsignedInt
  item_nome String? @db.VarChar(255)
  box_id    Int?    @db.UnsignedInt
  box       box?    @relation(fields: [box_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "item_box_id_foreign")

  @@index([box_id], map: "item_box_id_foreign")
}

model knex_migrations {
  id             Int       @id @default(autoincrement()) @db.UnsignedInt
  name           String?   @db.VarChar(255)
  batch          Int?
  migration_time DateTime? @db.Timestamp(0)
}

model knex_migrations_lock {
  index     Int  @id @default(autoincrement()) @db.UnsignedInt
  is_locked Int?
}
